{
  "name": "alpheios-selenium-test",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "dependencies": {
    "axios": "^0.19.2",
    "jest": "^25.1.0",
    "selenium-webdriver": "^4.0.0-alpha.5"
  },
  "devDependencies": {},
  "scripts": {
    "test": "jest",
    "test-browserstack": "jest/browserstack/**.test.js --env=node --maxConcurrency=2",
    "test-latin-simple-lookup": "jest tests/browserstack/latin-simple-lookup.test.js --env=node --maxConcurrency=2",
    "test-greek-simple-lookup": "jest tests/browserstack/greek-simple-lookup.test.js --env=node --maxConcurrency=2",
    "test-ara-simple-lookup": "jest tests/browserstack/ara-simple-lookup.test.js --env=node --maxConcurrency=2",
    "test-per-simple-lookup": "jest tests/browserstack/per-simple-lookup.test.js --env=node --maxConcurrency=2",
    "test-gez-simple-lookup": "jest tests/browserstack/gez-simple-lookup.test.js --env=node --maxConcurrency=2",
    "test-latin-no-result-lookup": "jest tests/browserstack/latin-no-result-lookup.test.js --env=node --maxConcurrency=2",
    "test-latin-dblclick-lookup": "jest tests/browserstack/latin-dblclick-lookup.test.js --env=node --maxConcurrency=2",
    
    "update-browsers": "node src/update-browser-list.js"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^@src[/](.+)": "<rootDir>/src/$1",
      "^@tests[/](.+)": "<rootDir>/tests/$1"
    }
  }
}
